import { list } from "./input"
import List from "list"

let mut increases = 0
let checkIncreases = (curr, index) => {
  match (List.nth(index - 1, list)) {
    None => void,
    Some(prev) => {
      if (curr > prev) { increases += 1 }
    }
  }
}
List.forEachi(checkIncreases, list)

print(increases)
